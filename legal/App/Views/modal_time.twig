<script>
    $(document).on("click", "#btn-sm", function () {
        var nombre = $(this).data('nom');
        var destino = $(this).data('dest');
        $("#nombre").val(nombre);
        $("#destino").val(destino);
    })

    $(document).on("click", "#btn-del", function () {
        var ide = $(this).data('id');       
