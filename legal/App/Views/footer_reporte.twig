  $(function () {
    /* ChartJS
     * -------
     * Here we will create a few charts using ChartJS
     */    

    //-------------
    //- LINE CHART -
    //--------------
    var lineChartCanvas = $('#lineChart').get(0).getContext('2d')
    var lineChartOptions1 = {
      maintainAspectRatio : false,
      responsive : true,
      legend: {
        display: true
      },
      animations: {
      tension: {
        duration: 1000,
        easing: 'linear',
        from: 1,
        to: 0,
        loop: true
      }
        },
      scales: {
        xAxes: [{
          gridLines : {
            display : true,
          },
          scaleLabel: {
              display : true,
              labelString    : 'Torneos'
          }
        }],
        yAxes: [{
          gridLines : {
            display : true,
          },
          scaleLabel: {
              display : true,
              labelString    : 'Tiempo en Segundos'
          }
        }]
      }
    }

    let url = '{{ ruta }}/reportes/2/getHistorial'
    //let url = 'https://jsonplaceholder.typicode.com/todos/'
    fetch(url)
        .then( response => response.json() )
        .then( datos => mostrar(datos) )
        .catch( error => console.log(error) )

    const mostrar = (datos) =>{
            datos.forEach(element => {
                lineChart.data['labels'].push(element.torneo)
                lineChart.data['datasets'][0].data.push(element.segundos)
                lineChart.update()
            });
            console.log(lineChart.data)
        }    

    var lineChartData = {
     
      datasets: [
        {
          label               : '{{ deportista.apellido }}',
          tension             : 0,
          pointStyle          : 'circle',          
          backgroundColor     : 'rgba(60,141,188,0.9)',
          borderColor         : 'rgba(60,141,188,0.8)',          
          pointColor          : '#3b8bba',
          pointStrokeColor    : 'rgba(60,141,188,1)',
          pointHighlightFill  : '#fff',
          pointStyle            : 'circle',
          pointRadius           : 5,
          pointHoverRadius      : 15,
          pointHighlightStroke: 'rgba(60,141,188,1)'     
        },
        
      ]
    }
    lineChartData.datasets[0].fill = false;
    //lineChartData.datasets[1].fill = false;
    lineChartOptions1.datasetFill = false

    var lineChart = new Chart(lineChartCanvas, {
      type: 'line',
      data: lineChartData,
      options: lineChartOptions1      
    })
  })
   
