<script>
        $(document).ready(function(){
        // Bloqueamos el SELECT de los cursos
        $("#club").prop('disabled', true);

    // Hacemos la l√≥gica que cuando nuestro SELECT cambia de valor haga algo
    $("#liga").change(function(){
        // Guardamos el select de cursos
        var clubes = $("#club");

        // Guardamos el select de alumnos
        var ligas = $(this);

        var id = ligas.val();

        if($(this).val() != '')
        {
            $.ajax({
                data: { id : ligas.val() },
                url:   '{{ ruta }}/Reportes/'+id+'/findClubes',
                type:  'POST',
                dataType: 'json',
                beforeSend: function () 
                {
                    ligas.prop('disabled', true);
                },
                success:  function (r) 
                {
                    ligas.prop('disabled', false);

                    // Limpiamos el select
                    clubes.find('option').remove();

                    clubes.append('<option></option>');

                    $(r).each(function(i, v){ // indice, valor
                        clubes.append('<option value="' + v.id + '">'+v.nombre_club+ '</option>');
                    })

                    clubes.prop('disabled', false);
                },
                error: function()
                {
                    alert('Ocurrio un error en el servidor ..');
                    ligas.prop('disabled', false);
                }
            });
        }
        else
        {
            clubes.find('option').remove();
            clubes.prop('disabled', true);
        }
    })
 })
 </script>

